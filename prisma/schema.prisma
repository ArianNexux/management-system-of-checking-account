// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DEV_DATABASE_URL")
}

model Instituition {
  id String @id @unique

  name String

  logo String

  title1 String

  title2 String

  heightLogo String

  widthLogo String

  @@map("instituition")
}

model Employee {
  id String @id @unique

  email String @unique

  name String

  photo String

  role String

  position String

  @@map("employees")
}
